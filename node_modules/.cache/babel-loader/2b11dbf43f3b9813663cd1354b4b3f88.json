      name=\"semester\"\r\n                                                type=\"checkbox\"\r\n                                                id=\"after\"\r\n                                                value=\"後期\"\r\n                                                checked={semester_option.includes(\"後期\")}\r\n                                                onChange={semester_change}\r\n                                            />\r\n                                            <Form.Check\r\n                                                inline\r\n                                                label=\"通年\"\r\n                                                type=\"checkbox\"\r\n                                                id=\"year\"\r\n                                                value=\"通年\"\r\n                                                checked={semester_option.includes(\"通年\")}\r\n                                                onChange={semester_change}\r\n                                            />\r\n                                        </div>\r\n                                    </td>\r\n                                </tr>\r\n                                <tr key=\"period\">\r\n                                    <th>\r\n                                        曜日・時限\r\n                                    </th>\r\n                                    <td>\r\n                                        <Button onClick={() => setButtonState(buttonOn => !buttonOn)}>\r\n                                            曜日・時限を選択\r\n                                        </Button>\r\n                                        <div id=\"timetable\" className={`${buttonOn ? 'tt_show' : 'tt_close'}`}>\r\n                                            <table>\r\n                                                <thead>\r\n                                                    <tr key=\"day\">\r\n                                                        <th></th>\r\n                                                        <th>\r\n                                                            月\r\n                                                        </th>\r\n                                                        <th>\r\n                                                            火\r\n                                                        </th>\r\n                                                        <th>\r\n                                                            水\r\n                                                        </th>\r\n                                                        <th>\r\n                                                            木\r\n                                                        </th>\r\n                                                        <th>\r\n                                                            金\r\n                                                        </th>\r\n                                                    </tr>\r\n                                                </thead>\r\n                                                <tbody>\r\n                                                    <tr key=\"1\">\r\n                                                        <th>\r\n                                                            1・2\r\n                                                        </th>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"月1,月2\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"火1,火2\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"水1,水2\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"木1,木2\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"金1,金2\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                    </tr>\r\n                                                    <tr key=\"2\">\r\n                                                        <th>\r\n                                                            3・4\r\n                                                        </th>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"月3,月4\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"火3,火4\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"水3,水4\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"木3,木4\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"金3,金4\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                    </tr>\r\n                                                    <tr key=\"3\">\r\n                                                        <th>\r\n                                                            5・6\r\n                                                        </th>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"月5,月6\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"火5,火6\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"水5,水6\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"木5,木6\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"金5,金6\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                    </tr>\r\n                                                    <tr key=\"4\">\r\n                                                        <th>\r\n                                                            7・8\r\n                                                        </th>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"月7,月8\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"火7,火8\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"水7,水8\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"木7,木8\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"金7,金8\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                    </tr>\r\n                                                    <tr key=\"5\">\r\n                                                        <th>\r\n                                                            9・10\r\n                                                        </th>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"月9,月10\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"火9,火10\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"水9,水10\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"木9,木10\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                        <td>\r\n                                                            <Form.Check\r\n                                                                name=\"period\"\r\n                                                                value=\"金9,金10\"\r\n                                                                type=\"checkbox\"\r\n                                                                onChange={period_change}\r\n                                                            />\r\n                                                        </td>\r\n                                                    </tr>\r\n                                                </tbody>\r\n                                            </table>\r\n                                            <Form.Check\r\n                                                label=\"集中講義\"\r\n                                                name=\"period\"\r\n                                                value=\"集中\"\r\n                                                type=\"checkbox\"\r\n                                                onChange={period_change}\r\n                                            />\r\n                                        </div>\r\n                                    </td>\r\n                                </tr>\r\n                                <tr key=\"teacher\">\r\n                                    <th>\r\n                                        教員名\r\n                                    </th>\r\n                                    <td>\r\n                                        <InputGroup onChange={(e) => setTeacher_name(e.target.value)}>\r\n                                            <FormControl />\r\n                                        </InputGroup>\r\n                                    </td>\r\n                                </tr>\r\n                            </tbody>\r\n                        </Table>\r\n                    </Accordion.Body>\r\n                </Accordion.Item>\r\n            </Accordion>\r\n            {display_table}\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default Search;"]},"metadata":{},"sourceType":"module"}